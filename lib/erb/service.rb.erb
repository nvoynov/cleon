require_relative '../basics/service'

module <%= @model.root_const %>
  module Services

    class <%= @model.const %> < Service

      def initialize(<%= @model.keyword_arguments %>)
<% for param in @model.params -%>
<% if param.guard -%>
        @<%= param.name %> = Guard<%= param.guard.capitalize %>.(<%= param.name %>)
<% else -%>
        @<%= param.name %> = <%= param.name %>
<% end -%>
<% end -%>
      end

      def call
        # gateway.<%= @model.name %>(<%= @model.at_arguments %>)
      end

    end

  end
end
